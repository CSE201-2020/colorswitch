<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="mainRoot" prefHeight="700.0" style="-fx-background-color: #252525;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <children>
        <ImageView fx:id="startGame" fitHeight="500.0" fitWidth="400.0" layoutY="136.0" onMouseClicked="#startGame" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../assets/color-switchmainmenu.jpg" />
            </image>
        </ImageView>

        <ImageView fx:id="loadGame" fitHeight="109.0" fitWidth="200.0" layoutX="200.0" layoutY="589.0" onMouseClicked="#loadGame" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../assets/loadgame.png" />
            </image>
        </ImageView>
    </children>

</AnchorPane>
